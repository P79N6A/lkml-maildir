[7169377:10305891759] -A INPUT -m state --state RELATED,ESTABLISHED -j ACCEPT
[43:2598] -A INPUT -p icmp -j ACCEPT
[3964:5136292] -A INPUT -m state --state INVALID -j LOG --log-prefix
"INVALID packet: "
