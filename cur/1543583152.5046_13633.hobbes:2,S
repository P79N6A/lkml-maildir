--- linux-2.4.2/drivers/net/tokenring/tms380tr.c	Fri Feb 16 22:02:36 2001
+++ linux-2.4.2.acme/drivers/net/tokenring/tms380tr.c	Mon Feb 26 23:11:51 2001
@@ -2203,6 +2203,7 @@
 				skb_trim(skb,Length);
 				skb->protocol = tr_type_trans(skb,dev);
 				netif_rx(skb);
+				dev->last_rx = jiffies;
 			}
 		}
 		else	/* Invalid frame */
