        { "VT 82C595 Apollo VP2",       PCI_DEVICE_ID_VIA_82C595, },
        { "VT 82C597 Apollo VP3",       PCI_DEVICE_ID_VIA_82C597_0, },
        { "VT 82C598 Apollo MVP3",      PCI_DEVICE_ID_VIA_82C598_0, },
