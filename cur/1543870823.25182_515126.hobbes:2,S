> --- a/drivers/usb/serial/usb-serial.c	2007-03-03 16:28:22.000000000 +0100
> +++ b/drivers/usb/serial/usb-serial.c	2007-03-03 16:28:26.000000000 +0100
> @@ -847,7 +847,6 @@
>  		port = kzalloc(sizeof(struct usb_serial_port), GFP_KERNEL);
