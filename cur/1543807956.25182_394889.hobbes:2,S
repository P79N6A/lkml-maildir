@@ -26,6 +26,10 @@ config ARCH_MAY_HAVE_PC_FDC
  	depends on Q40 || (BROKEN && SUN3X)
  	default y
+config ARCH_HAS_PFN_TO_PAGE
+	bool
+	default y
+
  mainmenu "Linux/68k Kernel Configuration"
  source "init/Kconfig"
