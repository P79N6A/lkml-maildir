\  /\  /\  /  Vince Weaver          
 \/__\/__\/   weave@eng.umd.edu     
http://www.glue.umd.edu/~weave
--- ./linux/drivers/char/drm/drmP.orig.h        Wed Mar 15 21:04:22 2000
+++ ./linux/drivers/char/drm/drmP.h     Wed Mar 15 21:21:20 2000
@@ -50,7 +50,6 @@
 #include <asm/mtrr.h>
 #endif
 #if LINUX_VERSION_CODE >= KERNEL_VERSION(2,1,0)
-#include <asm/spinlock.h>
 #include <linux/poll.h>
 #endif
 #include "drm.h"
