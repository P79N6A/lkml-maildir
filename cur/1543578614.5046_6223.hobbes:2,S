Should be +EXTRAVERSION =-pre3 instead of -pre2
diff -u --recursive --new-file v2.4.0/linux/Makefile linux/Makefile
@@ -1,7 +1,7 @@
 VERSION = 2
 PATCHLEVEL = 4
-SUBLEVEL = 0
-EXTRAVERSION =
+SUBLEVEL = 1
+EXTRAVERSION =-pre2
