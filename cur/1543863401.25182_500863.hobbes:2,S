+++ linux-2.6.19/arch/arm/mach-imx/time.c
@@ -87,6 +87,7 @@ static struct clocksource clocksource_im
 	.read		= imx_get_cycles,
 	.mask		= 0xFFFFFFFF,
 	.shift 		= 20,
+	.list		= LIST_HEAD_INIT(clocksource_imx.list),
 	.is_continuous 	= 1,
 };
 
