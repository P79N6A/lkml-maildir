> > +static inline void dput_path(struct path *path, struct nameidata *nd)
> > +{
> > +	dput(path->dentry);
> > +	if (path->mnt != nd->mnt)
> > +		mntput(path->mnt);
> > +}
> > +
> > +static inline void path_to_nameidata(struct path *path, struct nameidata *nd)
> > +{
> > +	dput(nd->dentry);
> > +	if (nd->mnt != path->mnt)
> > +		mntput(nd->mnt);
> > +	nd->mnt = path->mnt;
> > +	nd->dentry = path->dentry;
> > +}
