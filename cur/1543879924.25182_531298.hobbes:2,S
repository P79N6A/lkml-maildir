[7]. Program which triggers error:
/*
 *  cpucontrol.c
 */
#include <linux/kernel.h>
#include <linux/module.h>
#include <linux/fs.h>
#include <linux/kobject.h>
#include <linux/cpu.h>
#include <linux/sysfs.h>
#include <asm/uaccess.h>
/*
 * sysfs setup structures
 */
/* Handlers for each sysfs entry */
struct control_attr {
        struct attribute attr;
        ssize_t (*show)(char *);
        ssize_t (*store)(const char *, size_t count);
};
static int module_enabled_writes = 0;
/* By default on Linux alignment mask is enabled */
#define CR0_AM  0x40000 /* alignment mask */
static ssize_t show_alignment_checks(char *buf)
{
	/*
	 * Are alignment checks on?
	 * See Intel 64 and IA-32 Architectures Software Developer's Manual
	 * Volume 3A: System Programming Guide, Part 1
	 * 2.5 Control Registers , Alignment Mask
	 *
	 */
	const unsigned long cr0_am = read_cr0() & (CR0_AM);
	return sprintf(buf, "%s\n", cr0_am ? "yes" : "no");
}
static ssize_t show_module_enabled(char *buf)
{
	return sprintf(buf,"%s\n", module_enabled_writes ? "enabled" : "disabled");
}
static ssize_t store_module_enabled(const char *buf, size_t count)
{
	unsigned int enable = 0;
	int ret;
	ret = sscanf(buf, "%d", &enable);
	if(ret != 1)
		return -EINVAL;
	if(module_enabled_writes != enable) {
		printk(KERN_INFO "%s CPU control module writes\n", enable ?
"Enabling" : "Disabling");
	}
	module_enabled_writes = enable;
	return count;
}
static ssize_t store_wbinvd_execute(const char *buf, size_t count)
{
	unsigned int execute = 0;
	int ret;
	/*
	 * You first need to enable the module writes, by writing to
	 * module_enabled sysfs file
	 */
	if(!module_enabled_writes) {
		return -EACCES;
	}
	ret = sscanf(buf,"%d",&execute);
	if(ret != 1)
		return -EINVAL;
	if(execute) {
		/*printk(KERN_INFO "Executing wbinvd\n");*/
		/*
		 * Write back and invalidate cache
		 */
		wbinvd();
	}
	return count;
}
static ssize_t store_alignment_checks(const char *buf, size_t count)
{
	unsigned long new_cr0;
	unsigned int enable = 0;
	int ret;
	/*
	 * You first need to enable the module writes, by writing to
	 * module_enabled sysfs file
	 */
	if(!module_enabled_writes) {
		return -EACCES;
	}
	ret = sscanf(buf, "%d", &enable);
	if(ret != 1)
		return -EINVAL;
	/*
	 * Enable/disable alignment checks
	 * See Intel 64 and IA-32 Architectures Software Developer's Manual
	 * Volume 3A: System Programming Guide, Part 1
	 * Chapter 5, Interrupt 17 - Alignment Check Exception (#AC)
	 *
	 * User programs will still need to explicitly enable alignment checks
	 * by setting the AC bit in their EFLAGS register.
	 *
	 * Alignment check exception will only occur on CPL 3, thus kernel will
	 * be unaffected.
	 */
	new_cr0 = (read_cr0() & (~CR0_AM)) | (enable ? CR0_AM : 0);
	printk(KERN_INFO "%s alignment checks\n", enable ? "Enabling" : "Disabling");
	write_cr0(new_cr0);
	return count;
}
#define define_one_rw0600(_name) \
static struct control_attr _name = \
__ATTR(_name, 0644, show_##_name, store_##_name)
#define define_one_rw(_name) \
static struct control_attr _name = \
__ATTR(_name, 0644, show_##_name, store_##_name)
#define define_one_wo(_name) \
static struct control_attr _name = \
__ATTR(_name, 0200, NULL, store_##_name)
#define define_one_ro0400(_name) \
static struct control_attr _name = \
__ATTR(_name, 0400, show_##_name, NULL)
define_one_wo(wbinvd_execute);
define_one_rw(alignment_checks);
define_one_rw0600(module_enabled);
/* Attributes changeable via sysfs */
static struct attribute * default_attrs[] = {
	&module_enabled.attr,
	&wbinvd_execute.attr,
	&alignment_checks.attr
};
#define to_attr(a) container_of(a, struct control_attr, attr)
static ssize_t show(struct kobject * kobj, struct attribute * attr, char * buf)
{
	struct control_attr * cattr = to_attr(attr);
	ssize_t ret;
	if(cattr->show)
		ret = cattr->show(buf);
	else
		ret = -EIO;
	return ret;
}
static ssize_t store(struct kobject * kobj, struct attribute * attr,
		const char * buf, size_t count)
{
	struct control_attr * cattr = to_attr(attr);
	ssize_t ret;
	if(cattr->store)
		ret = cattr->store(buf, count);
	else
		ret = -EIO;
	return ret;
}
static struct sysfs_ops sysfs_ops = {
	.show = show,
	.store = store
};
static struct kobj_type ktype_percpu_entry = {
	.sysfs_ops = &sysfs_ops,
	.default_attrs = default_attrs
};
static struct kobject* control_kobject[NR_CPUS];
static int control_add_dev(struct sys_device* sysdev)
{
	const unsigned int cpu = sysdev->id;
	int rc;
        control_kobject[cpu] = kzalloc(sizeof(control_kobject[0]), GFP_KERNEL);
	if(unlikely(control_kobject[cpu] == NULL))
		return -ENOMEM;
	control_kobject[cpu]->parent = &sysdev->kobj;
	kobject_set_name(control_kobject[cpu], "%s", "control");
	control_kobject[cpu]->ktype = &ktype_percpu_entry;
	rc = kobject_register(control_kobject[cpu]);
	if(unlikely(rc)) {
		kfree(control_kobject[cpu]);
		control_kobject[cpu] = NULL;
	}
	return rc;
}
static void control_remove_dev(struct sys_device * sysdev)
{
	const unsigned int cpu = sysdev->id;
	kobject_unregister(control_kobject[cpu]);
	kfree(control_kobject[cpu]);
	control_kobject[cpu] = NULL;
}
/*
 *
 * Module startup & cleanup
 *
 */
/*
 * This function is called when the module is loaded
 */
static int __init our_init_module(void)
{
	struct sys_device* sysdev;
	int cpu, rc;
	printk(KERN_INFO "before for\n");
	/*
	 * add a control directory in sysfs under /sys/devices/system/cpu/cpuX/
	 */
	for_each_online_cpu(cpu) {
		printk(KERN_INFO "inside for_each\n");
		sysdev = get_cpu_sysdev(cpu);
		printk(KERN_INFO "after get_cpu\n");
		rc = control_add_dev(sysdev);
		printk(KERN_INFO "after control_add\n");
		if(unlikely(rc))
			return rc;
	}
	printk(KERN_INFO "CPU control directories added\n");
	return 0;
}
/*
 * This function is called when the module is unloaded
 */
static void __exit our_cleanup_module(void)
{
	unsigned int cpu;
	struct sys_device* sysdev;
	/*
	 * Unregister the sysfs entries
	 */
	for_each_online_cpu(cpu) {
		sysdev = get_cpu_sysdev(cpu);
		control_remove_dev(sysdev);
	}
	printk(KERN_INFO "CPU control directories unregistered\n");
}
