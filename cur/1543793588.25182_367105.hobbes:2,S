>                          Pekka
> 
> Index: 2.6/arch/i386/kernel/traps.c
> ===================================================================
> --- 2.6.orig/arch/i386/kernel/traps.c
> +++ 2.6/arch/i386/kernel/traps.c
> @@ -185,8 +185,10 @@ void show_stack(struct task_struct *task
>  			printk("\n       ");
>  		printk("%08lx ", *stack++);
>  	}
> +#if 0
>  	printk("\nCall Trace:\n");
>  	show_trace(task, esp);
> +#endif
>  }
