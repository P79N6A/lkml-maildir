>         return ret;
>  }
> 
> @@ -1691,9 +1692,10 @@
> 
>         ret = nand_do_write_ops(mtd, to, &chip->ops);
> 
> +       *retlen = chip->ops.retlen;
> +
>         nand_release_device(mtd);
