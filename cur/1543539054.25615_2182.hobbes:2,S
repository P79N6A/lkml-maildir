 L_OBJS   := errno.o ctype.o string.o vsprintf.o brlock.o
 LX_OBJS  := cmdline.o
 
-ifneq ($(CONFIG_HAVE_DEC_LOCK),y) 
+ifeq ($(CONFIG_HAVE_DEC_LOCK),y) 
   L_OBJS += dec_and_lock.o
 endif
 