> +    # c library 5
> +    if ( -e "/lib/libc.so.5" ) {
> +	( $v_libc5 = `/lib/libc.so.5`) =~ m/GNU C Library .+ version (\S+),/;
> +	$v_libc5 = $1;
> +    } else {
> +	$v_libc5 = "not found";
