#include <linux/slab.h>
#include <linux/interrupt.h>  
#define MEMORY_TABLE_SIZE	512
void *
my_kmalloc(unsigned int size, char *id)
{
	char *ptr ;
	if ((ptr =
	     kmalloc(size,
		     (in_interrupt()? GFP_ATOMIC : GFP_KERNEL))) == NULL) {
		kprintf("Cannot allocate %u bytes for %s\n", size, id);
	}
}