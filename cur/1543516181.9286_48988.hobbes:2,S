diff -u -r linux-2.3.28/fs/ext2/acl.c linux/fs/ext2/acl.c
--- linux-2.3.28/fs/ext2/acl.c	Thu Oct 28 13:23:10 1999
+++ linux/fs/ext2/acl.c	Fri Nov  5 00:37:35 1999
@@ -7,7 +7,6 @@
  * Universite Pierre et Marie Curie (Paris VI)
  */
 
