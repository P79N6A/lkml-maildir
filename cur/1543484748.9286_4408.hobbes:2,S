> > 470     static inline int detach_timer(struct timer_list *timer)
> > 471     {
> > 472             struct timer_list *prev = timer->prev;
> > 473             if (prev) {
> > 474                     struct timer_list *next = timer->next;
> > 475                     prev->next = next;
> > 476                     if (next)
> > 477                             next->prev = prev;
> > 478                     return 1;
> > 479             }
