|> +#endif
|> +		     ,(flags & RTC_DST_EN) ? "yes" : "no",
|>  		     (flags & RTC_DM_BINARY) ? "no" : "yes",
|>  		     (flags & RTC_24H) ? "yes" : "no",
|>  		     (flags & RTC_SQWE) ? "yes" : "no",
|>  		     (flags & RTC_AIE) ? "yes" : "no",
|>  		     irq_active ? "yes" : "no",
|>  		     (flags & RTC_PIE) ? "yes" : "no",
|> -		     0L /* freq */,
|> -		     (flags & RTC_BATT_BAD) ? "bad" : "okay");
