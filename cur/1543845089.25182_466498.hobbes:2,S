@@ -1225,7 +1225,7 @@
     }
 
     if ( !(dc = get_dc_by_pdev(dev_id)) )  {
-        ERR("Could not find device context from pci_dev: %d", (u32) dev_id);
+        ERR("Could not find device context from pci_dev: %p", dev_id);
         return IRQ_NONE;
     }
 
