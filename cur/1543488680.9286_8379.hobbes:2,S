This is my conf.modules
alias           eth0            3c59x
alias           net-pf-4        off
alias           net-pf-5        off
alias           char-major-14   sb
post-install    sb              /sbin/modprobe "adlib_card"
options         sb              io=0x220 irq=5 dma=1 dma16=5
mpu_io=0x330
options         adlib_card      io=0x388
