@@ -74,6 +74,7 @@ static dev_t __init try_name(char *name,
 	/*
 	 * The format of dev is now %u:%u -- see print_dev_t()
 	 */
+	printk("scanning `%s'\n", buf);
 	if (sscanf(buf, "%u:%u", &maj, &min) == 2)
 		res = MKDEV(maj, min);
 	else
