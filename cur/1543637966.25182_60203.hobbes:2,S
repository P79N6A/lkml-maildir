Rusty.
--- working-2.5.29-upfix/init/main.c.~1~	Tue Jul 30 16:27:55 2002
+++ working-2.5.29-upfix/init/main.c	Tue Jul 30 16:28:29 2002
@@ -529,7 +529,9 @@
 	extern int migration_init(void);
 	extern int spawn_ksoftirqd(void);
 
+#ifdef CONFIG_SMP
 	migration_init();
+#endif
 	spawn_ksoftirqd();
 }
 
