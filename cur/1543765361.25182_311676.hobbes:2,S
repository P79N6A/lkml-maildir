>  #include <openssl/sha.h>
>  #include <zlib.h>
> 
> +#include <netinet/in.h>
> +#define cpu_to_beuint(x)	(htonl(x))
> +#define beuint_to_cpu(x)	(ntohl(x))
> +#define cpu_to_beushort(x)	(htons(x))
> +#define beushort_to_cpu(x)	(ntohs(x))
> +
