following?
 make zlilo
gcc -D__KERNEL__ -I/usr/src/linux/include -Wall -Wstrict-prototypes -O2
-fomit-frame-pointer -fno-strict-aliasing -D__SMP__ -pipe
-fno-strength-reduce  -DCPU=686 -march=i686  -c -o init/main.o
init/main.c
