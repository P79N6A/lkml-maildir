+   } while (0)
 
 #define DMWARN(f, arg...) \
    printk(KERN_WARNING DM_NAME ": " DM_MSG_PREFIX ": " f "\n", ## arg)
@@ -34,7 +34,7 @@
        if (printk_ratelimit()) \
