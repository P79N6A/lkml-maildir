+++ linux-2.6.11-rc3-mm1-full/drivers/mca/mca-legacy.c	2005-02-06 14:22:44.000000000 +0100
@@ -180,7 +180,6 @@
 
 	return info.mca_dev;
 }
-EXPORT_SYMBOL(mca_find_device_by_slot);
 
