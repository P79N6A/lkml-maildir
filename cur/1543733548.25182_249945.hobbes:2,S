+++ linux-2.6.9-rc1-bk14/arch/ppc/Makefile	2004-09-07 21:50:54.249282101 +0200
@@ -131,4 +131,6 @@ endif
 	@true
 
 CLEAN_FILES +=	include/asm-$(ARCH)/offsets.h \
-		arch/$(ARCH)/kernel/asm-offsets.s
+		arch/$(ARCH)/kernel/asm-offsets.s \
+		$(TOUT)
+		
