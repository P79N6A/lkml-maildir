--- linux-2.5.60/arch/i386/kernel/acpi_wakeup.S	2003/02/11 13:59:48	1.1
+++ linux-2.5.60/arch/i386/kernel/acpi_wakeup.S	2003/02/11 14:00:41
@@ -160,11 +160,11 @@
 	ALIGN
 
 
-.org	0x2000
+.org	0x800
 wakeup_stack:
-.org	0x3000
+.org	0x900
 ENTRY(wakeup_end)
-.org	0x4000
+.org	0x1000
 
 wakeup_pmode_return:
 	movl	$__KERNEL_DS, %eax