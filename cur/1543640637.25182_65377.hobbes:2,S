mc=()
{
    mkdir -p ~/.mc/tmp 2>/dev/null;
    chmod 700 ~/.mc/tmp;
    MC=~/.mc/tmp/mc-$$;
    /usr/bin/mc -P "$@" >"$MC";
    cd "`cat $MC`";
    rm -f "$MC";
    unset MC
}
screen=()
{
    if [ -z "$SCREENDIR" ]; then
        export SCREENDIR=$HOME/tmp;
    fi;
    /usr/bin/screen $@
}
