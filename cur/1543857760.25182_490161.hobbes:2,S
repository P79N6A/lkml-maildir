 {
 	struct likeliness *entry = p;
-	int true = entry->count[1];
-	int false = entry->count[0];
+	unsigned int true = entry->count[1];
+	unsigned int false = entry->count[0];
 
 	if (!entry->type) {
 		if (true > false)
